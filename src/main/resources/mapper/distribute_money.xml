<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.help.dao.Distribute_MoneyDao">
    <select id="findMoney" resultType="com.help.entity.Distribute_Money">
        select * from distribute_money
    </select>
    <!--按行政区划查询资金发放-->
    <select id="findMoneyByNp" resultType="com.help.entity.Distribute_Money">
        select * from distribute_money where native_place LIKE #{a}
    </select>
    <!--添加任务-->
    <insert id="addwork" parameterType="com.help.entity.Distribute_Money">
        insert into distribute_money(money_id,native_place,money_shiDistribute,shi,money_xianDistribute,xian,money_xiangDistribute,xiang,money_personGet,money_Getsum,dissribute_DATE) values(#{money_id},#{native_place},#{money_shiDistribute},#{shi},#{money_xianDistribute},#{xian},#{money_xiangDistribute},#{xiang},#{money_personGet},#{money_Getsum},#{dissribute_DATE})
    </insert>
    <!--修改任务-->
    <update id="editwork" parameterType="com.help.entity.Distribute_Money">
        update  distribute_money set native_place=#{native_place},money_shiDistribute=#{money_shiDistribute},shi=#{shi},money_xianDistribute=#{money_xianDistribute},xian=#{xian},money_xiangDistribute=#{money_xiangDistribute},xiang=#{xiang},money_personGet=#{money_personGet},money_Getsum=#{money_Getsum},dissribute_DATE=#{dissribute_DATE} where money_id=#{money_id}
    </update>
    <!--删除组织-->
    <delete id="removeMoneyById">
        delete from distribute_money where money_id in
        <foreach collection="list" item="x" open="(" separator="," close=")">
            #{x}
       </foreach>
    </delete>
</mapper>